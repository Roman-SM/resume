<section class="p-5">
  <p><b>{{name}}</b></p>
  <p><b>Users:</b></p>
  <ul>
    {{#each users}}
    <li>
      <p><b>Id: </b>{{id}}</p>
    </li>
    <li>
      <p><b>Name: </b>{{name}}</p>
    </li>
    <li>
      <p><b>Gender: </b>{{gender}}</p>
    </li>
    <li>
      <p><b>Age: </b>{{age}}</p>
    </li>
    <p><b>Friends:</b></p>
    <ul>
      {{#each friends}}
      <li>
        <p><b>Id: </b>{{id}}</p>
      </li>
      <li>
        <p><b>Name: </b>{{name}}</p>
      </li>
      <li>
        <p><b>Gender: </b>{{gender}}</p>
      </li>
      <li>
        <p><b>Age: </b>{{age}}</p>
      </li>
      <p><b>Mutual friends:</b></p>
      <ul>
        {{#each mutual_friends}}
        <li>
          <p><b>Id: </b>{{id}}</p>
        </li>
        <li>
          <p><b>Name: </b>{{name}}</p>
        </li>
        <li>
          <p><b>Gender: </b>{{gender}}</p>
        </li>
        <li>
          <p><b>Age: </b>{{age}}</p>
        </li>
        {{/each}}
      </ul>
      {{/each}}
    </ul>
    <p><b>Groups:</b></p>
    <ul>
      {{#each groups}}
      <li>
        <p><b>Id: </b>{{id}}</p>
      </li>
      <li>
        <p><b>Name: </b>{{name}}</p>
      </li>
      <li>
        <p><b>Description: </b>{{description}}</p>
      </li>
      <p><b>Members:</b></p>
      <ul>
        {{#each members}}
        <li>
          <p><b>Id: </b>{{id}}</p>
        </li>
        <li>
          <p><b>Name: </b>{{name}}</p>
        </li>
        <li>
          <p><b>Gender: </b>{{gender}}</p>
        </li>
        <li>
          <p><b>Age: </b>{{age}}</p>
        </li>
        {{/each}}
      </ul>
      {{/each}}
    </ul>
    {{/each}}
</section>