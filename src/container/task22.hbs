<main class="p-5">
  {{#with page_header}}
  <h1 class="text-center">{{title}}</h1>
  <p class="text-center">{{description}}</p>
  {{/with}}
  {{#with section}}
  <div class="card mb-5">
    <div class="card-body">
      {{#with about_us}}
      {{#with title_list}}
      <div class="display-6 text-end pb-3">{{main}}</div>
      <h2 class="text-end">{{sub}}</h2>
      {{/with}}
      <br>
      {{#with button}}
      <button class="btn btn-outline-info">
        {{text}}
      </button>
      {{/with}}
      {{/with}}
    </div>
  </div>
  <div class="card mb-5">
    <div class="card-body">
      {{#with advantage}}
      <div class="display-5 pb-3">{{title}}</div>
      <div class="card mb-4">
        <div class="card-body">
          {{#with efficiency}}
          <div class="display-4 pb-3">{{title}}</div>
          <h6 class="pb-3">{{sub_title}}</h6>
          <p>{{description}}</p>
          <button class="btn btn-outline-warning">{{button}}</button>
          {{/with}}
        </div>
      </div>
      <div class="card mb-4">
        <div class="card-body">
          {{#with innovation}}
          <div class="display-4 text-center pb-3">{{title}}</div>
          <h6 class="text-center pb-3">{{sub_title}}</h6>
          <p class="text-center pb-3">{{description}}</p>
          <button class="btn btn-outline-warning">{{button}}</button>
          {{/with}}
        </div>
      </div>
      <div class="card">
        <div class="card-body">
          {{#with collaboration}}
          <div class="display-4 text-end pb-3">{{title}}</div>
          <h6 class="text-end pb-3">{{sub_title}}</h6>
          <p class="text-end pb-3">{{description}}</p>
          <button class="btn btn-outline-warning">{{button}}</button>
          {{/with}}
        </div>
      </div>
      {{/with}}
    </div>
  </div>
  <div class="card">
    <div class="card-body">
      {{#with agreement}}
      <div class="display-5 pb-3">{{title}}</div>
      <div class="card">
        <div class="card-body">
          {{#with info}}
          <h5>{{sub_title}}</h5>
          <div class="card pb-3 mt-3">
            <div class="card-body">
              {{#with detail}}
              <p>{{text}}</p>
              <button class="btn btn-success">{{button}}</button>
              {{/with}}
            </div>
          </div>
          {{/with}}
        </div>
      </div>
      {{/with}}
    </div>
  </div>
  {{/with}}
</main>