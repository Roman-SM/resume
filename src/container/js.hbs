<div>
  <section>
    <b>Name: {{name}}</b>
    <ul>
      <p>Description: {{description}}</p>
      <p><b>History: </b></p>
      <ul>
        {{#with history}}
        <li>
          <p>Year: {{year}}</p>
        </li>
        <li>
          <p>Founder: {{founder}}</p>
        </li>
        {{/with}}
      </ul>
      <p><b>Types: </b></p>
      <ul>
        {{#each types}}
        <li>
          <p>{{this}}</p>
        </li>
        {{/each}}
      </ul>
      <p><b>Syntax: </b></p>
      <ul>
        {{#with syntax}}
        <p><b>Variables:</b></p>
        <ul>
          {{#each variables}}
          <li>
            <p>{{this}}</p>
          </li>
          {{/each}}
        </ul>
        <p><b>Functions:</b></p>
        <ul>
          {{#each functions}}
          <li>
            <p>{{this}}</p>
          </li>
          {{/each}}
        </ul>
        <p><b>Conditionals:</b></p>
        <ul>
          {{#each conditionals}}
          <li>
            <p>{{this}}</p>
          </li>
          {{/each}}
        </ul>
        <p><b>Loops:</b></p>
        <ul>
          {{#each loops}}
          <li>
            <p>{{this}}</p>
          </li>
          {{/each}}
        </ul>
        <p><b>Classes:</b></p>
        <ul>
          {{#each classes}}
          <li>
            <p>{{this}}</p>
          </li>
          {{/each}}
        </ul>
        {{/with}}
      </ul>
      <p><b>Features: </b></p>
      <ul>
        {{#each features}}
        <li>
          {{this}}
        </li>
        {{/each}}
      </ul>
      <p><b>Libraries: </b></p>
      <ul>
        {{#each libraries}}
        <li>
          {{this}}
        </li>
        {{/each}}
      </ul>
      <p><b>Tools: </b></p>
      <ul>
        {{#each tools}}
        <li>
          {{this}}
        </li>
        {{/each}}
      </ul>
      <p><b>Ð¡ommunity: </b></p>
      <ul>
        {{#with community}}
        <li>
          <p>Name: {{[0].name}}</p>
        </li>
        <li>
          <p>Type: {{[0].type}}</p>
        </li>
        <br>
        <li>
          <p>Name: {{[1].name}}</p>
        </li>
        <li>
          <p>Type: {{[1].type}}</p>
        </li>
        <br>
        <li>
          <p>Name: {{[2].name}}</p>
        </li>
        <li>
          <p>Type: {{[2].type}}</p>
        </li>
        <br>
        <li>
          <p>Name: {{[3].name}}</p>
        </li>
        <li>
          <p>Type: {{[3].type}}</p>
        </li>
        {{/with}}
      </ul>
    </ul>
  </section>
</div>